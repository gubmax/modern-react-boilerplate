{
  "name": "modern-react-boilerplate",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=14.9.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gubmax/modern-react-boilerplate.git"
  },
  "author": "Maksim Gubyshev <gubyshev@gmail.com> (https://github.com/gubmax)",
  "scripts": {
    "dev": "ts-node server/main",
    "debug": "ts-node --inspect-brk server/main",
    "build": "npm run build:client && npm run build:server && npm run prerender",
    "build:client": "vite build --outDir dist/client",
    "build:server": "vite build --ssr server/renderClient --outDir dist/server",
    "prerender": "ts-node scripts/prerender",
    "serve": "cross-env NODE_ENV=production ts-node server/main",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx",
    "prepare": "husky install"
  },
  "dependencies": {
    "@nestjs/common": "8.0.6",
    "@nestjs/config": "1.0.1",
    "@nestjs/core": "8.0.6",
    "@nestjs/platform-express": "8.0.6",
    "@vanilla-extract/css": "1.6.0",
    "@vanilla-extract/sprinkles": "1.3.0",
    "compression": "1.7.4",
    "history": "5.0.1",
    "immer": "9.0.6",
    "node-fetch": "2.6.1",
    "pino": "6.13.2",
    "react": "18.0.0-alpha-a8cabb564-20210915",
    "react-dom": "18.0.0-alpha-a8cabb564-20210915",
    "react-router": "6.0.0-beta.4",
    "react-router-dom": "6.0.0-beta.4",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.3.0",
    "serve-static": "1.14.1",
    "web-vitals": "2.1.0"
  },
  "devDependencies": {
    "@swc/core": "1.2.88",
    "@types/compression": "1.7.2",
    "@types/node": "16.9.4",
    "@types/node-fetch": "2.5.12",
    "@types/pino": "6.3.11",
    "@types/react": "17.0.22",
    "@types/react-dom": "17.0.9",
    "@types/serve-static": "1.13.10",
    "@typescript-eslint/eslint-plugin": "4.31.2",
    "@typescript-eslint/parser": "4.31.2",
    "@vanilla-extract/vite-plugin": "1.1.2",
    "@vitejs/plugin-react-refresh": "1.3.6",
    "chalk": "4.1.2",
    "chokidar": "3.5.2",
    "cross-env": "7.0.3",
    "eslint": "7.32.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.25.3",
    "eslint-plugin-react-hooks": "4.2.0",
    "husky": "7.0.2",
    "lint-staged": "11.1.2",
    "prettier": "2.4.1",
    "ts-node": "10.2.1",
    "tsconfig-paths": "3.11.0",
    "typescript": "4.4.3",
    "vite": "2.5.10",
    "vite-plugin-pwa": "0.11.2"
  }
}
